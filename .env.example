# .env.example - Copy this file to .env and fill in your API keys
# At least one of OpenAI or Anthropic API keys is required

# OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API key
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Azure OpenAI API configuration (optional)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_API_VERSION=2023-05-15
AZURE_OPENAI_DEPLOYMENT_FAST=your-gpt35-deployment-name
AZURE_OPENAI_DEPLOYMENT_BALANCED=your-gpt4-deployment-name
AZURE_OPENAI_DEPLOYMENT_POWERFUL=your-gpt4o-deployment-name